includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    level: 8
    paths:
        - packages
    ignoreErrors:
        - '#Method (.*?) should return Illuminate\\Database\\Eloquent\\Collection but returns Illuminate\\Support\\Collection<(.*?)>.#'
        - '#Cannot access property \$pivot on (.*?)#'
        - '#Parameter \#1 \$array of function array_pop expects array, array<int, string>\|false given.#'
